<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_End_Date_Check</fullName>
    <active>true</active>
    <description>Start date cannot be greater than end date.</description>
    <errorConditionFormula>(PF_Actual_Start_Date__c   &gt;  PF_Actual_End_Date__c ) ||  (PF_Planned_Start_Date__c  &gt; PF_Planned_End_Date__c )</errorConditionFormula>
    <errorMessage>Start dates cannot be greater than end dates.</errorMessage>
</ValidationRule>
